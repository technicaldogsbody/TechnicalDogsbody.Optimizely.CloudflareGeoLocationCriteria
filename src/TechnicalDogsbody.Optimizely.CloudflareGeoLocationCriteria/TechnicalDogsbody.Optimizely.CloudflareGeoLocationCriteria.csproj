<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
	<Title>TechnicalDogsbody's Optimizely Cloudflare GeoLocation Criteria</Title>
	<Version>1.0.2</Version>
	<RepositoryType>git</RepositoryType>
	<RepositoryUrl>https://github.com/technicaldogsbody/TechnicalDogsbody.Optimizely.CloudflareGeoLocationCriteria</RepositoryUrl>
	<PackageTags>Optimizely; Personalisation; Visitor Groups</PackageTags>
  </PropertyGroup>

  <ItemGroup>
	<None Include="README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EPiServer.CMS.Core" Version="12.4.0" />
    <PackageReference Include="EPiServer.Personalization.Content.UI" Version="1.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
  </ItemGroup>

</Project>
